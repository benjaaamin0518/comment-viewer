{
  "name": "comment-viewer",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "scripts": {
    "stylelint": "stylelint --fix ./src/scss/**/*.scss",
    "serve": "cross-env NODE_ENV=\"development\"  webpack-dev-server --config ./webpack.config.js",
    "dev": "npm run serve & onchange './src/scss/**/*.scss' -- npm run stylelint & gulp",
    "build": "NODE_ENV=production webpack --colors --config ./webpack.config.js",
    "dev-electron": " rimraf dist && run-p dev:*",
    "build-electron-mac": "rimraf dist && npm run build:tsc && npm run build:webpack && electron-packager . --asar",
    "build-electron-win": "rimraf dist && npm run build:tsc && npm run build:webpack && electron-packager . --asar --platform=win32 --arch=x64",
    "dev:tsc": "tsc -w -p tsconfig.node.json",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --config ./webpack.app.config.js",
    "build:tsc": "tsc -p tsconfig.node.json",
    "build:webpack": "cross-env NODE_ENV=\"production\" NODE_OPTIONS='--openssl-legacy-provider' webpack --config ./webpack.app.config.js",
    "dev:electron": "wait-on ./dist/index.html ./dist/main.js && cross-env NODE_ENV=\"development\" electron ."
  },
  "repository": "https://github.com/benjaaamin0518/comment-viewer.git",
  "author": "benjaaamin0518",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-window": "^1.8.8",
    "@types/redux-logger": "^3.0.7",
    "@types/uuid": "^3.4.6",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^5.0.7",
    "stylelint": "^12.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.2",
    "wait-on": "^7.2.0",
    "webpack": "^4.47.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/styled-engine-sc": "^6.0.0-alpha.18",
    "core-js": "^3.6.0",
    "date-fns": "^2.8.1",
    "firebase": "^7.6.1",
    "gsap": "^3.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-window": "^1.8.10",
    "styled-components": "^6.1.11",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.1",
    "uuid": "^3.3.3",
    "whatwg-fetch": "^3.0.0"
  }
}
